{
	"name": "friendsoftypo3/taskcenter",
	"type": "typo3-cms-extension",
	"description": "The Task Center is the framework for a host of other extensions.",
	"homepage": "https://extensions.typo3.org/extension/taskcenter/",
	"support": {
		"docs": "https://docs.typo3.org/p/friendsoftypo3/taskcenter/main/en-us/",
		"issues": "https://github.com/FriendsOfTYPO3/taskcenter/issues",
		"source": "https://github.com/FriendsOfTYPO3/taskcenter"
	},
	"license": ["GPL-2.0-or-later"],
	"authors": [{
		"name": "TYPO3 Core Team",
		"email": "typo3cms@typo3.org",
		"role": "Developer"
	}],
	"config": {
		"sort-packages": true
	},
	"require": {
		"typo3/cms-core": "^11.5.23"
	},
	"conflict": {
		"typo3/cms": "*"
	},
	"replace": {
		"typo3-ter/taskcenter": "*",
		"typo3/cms-taskcenter": "*"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "taskcenter"
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Taskcenter\\": "Classes/"
		}
	}
}
